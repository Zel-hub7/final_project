    <div class="col-md-3">
      <div class="box p-3 mb-2 bg-light">Box 6</div>
    </div>
    <div class="col-md-3">
      <div class="box p-3 mb-2 bg-light">Box 7</div>
    </div>
    <div class="col-md-3">
      <div class="box p-3 mb-2 bg-light">Box 8</div>
    </div>
      </div>
   </div>

</div>



