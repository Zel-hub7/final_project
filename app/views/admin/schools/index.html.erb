<h1>Manage Schools</h1>

<%= link_to "New School", new_admin_school_path %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
    </tr>
  </thead>

  <tbody>
    <% @schools.each do |school| %>
      <tr>
        <td><%= school.name %></td>
        <td><%= school.address %></td>
      </tr>
    <% end %>
  </tbody>
</table>
